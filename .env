# Server Configuration
PORT=3000
NODE_ENV="development"

# Database Connection
DB_CONNECTION_STRING="mongodb://localhost:27017/ai-ipst"

# JWT Authentication
JWT_SECRET="your_secret_key_here"
JWT_EXPIRES_IN="24h"

# Proxy Server
PROXY_TARGET_HOSTNAME="example.com"
PROXY_TARGET_PORT=80

# Logging
LOG_LEVEL="info"
LOG_DIR="./logs"

# External API Keys (if applicable)
OPENAI_API_KEY="your_openai_api_key_here"
ANTHROPIC_API_KEY="your_anthropic_api_key_here"